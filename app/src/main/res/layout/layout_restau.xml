<?xml version="1.0" encoding="utf-8"?>

<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginBottom="30dp"
    android:padding="5dp"
    android:layout_width="fill_parent"
    android:layout_marginTop="20dp"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_marginTop="10sp"
        android:layout_marginBottom="20sp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="404dp"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <RelativeLayout
                    android:layout_width="91dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="67dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="11dp"
                        android:layout_weight="1"
                        android:elevation="10dp"
                        android:text="   Hours to Close"
                        android:textAlignment="center"
                        android:textSize="8sp" />

                    <ImageView
                        android:id="@+id/shine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        />

                    <ImageView
                        android:id="@+id/imageViewres"
                        android:layout_width="87dp"
                        android:layout_height="78dp"
                        android:layout_alignParentTop="true"


                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="4dp"

                        app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />


                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="77dp"
                        android:layout_height="64dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:elevation="10dp"
                        android:visibility="invisible"
                        />

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="122dp"
                        android:layout_height="25dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="3dp"
                        />

                    <TextView
                        android:id="@+id/textView32"
                        android:layout_width="88dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="11dp"
                        android:gravity="center_horizontal"
                        android:text="HUNGERBITE"
                        android:textAllCaps="true"
                        android:textSize="9dp"
                        android:textStyle="bold" />


                </RelativeLayout>


                <LinearLayout
                    android:layout_width="238dp"
                    android:layout_height="120dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewrname"
                        android:layout_width="199dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_marginLeft="84dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="3sp"
                        android:text="TextView"
                        android:textSize="12sp"
                        />

                    <TextView
                        android:id="@+id/textViewrcity"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginStart="8dp"

                        android:singleLine="true"
                        android:text="Ludhiana"
                        android:textSize="10sp" />


                    <LinearLayout
                        android:layout_width="106dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="3dp">

                        <TextView
                            android:id="@+id/textView2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="8dp"
                            android:paddingBottom="2sp"
                            android:paddingTop="2sp"
                            android:textSize="9sp" />

                        <TextView
                            android:id="@+id/textViewrmin"
                            android:layout_width="216dp"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="8dp"
                            android:layout_marginRight="8dp"
                            android:paddingTop="2sp"
                            android:text="TextView"
                            android:textSize="10sp"
                            />

                    </LinearLayout>

                    <TableRow
                        android:id="@+id/hr"
                        android:layout_width="match_parent"
                        android:layout_height="1sp"
                        android:background="#07000000">

                    </TableRow>

                    <LinearLayout
                        android:id="@+id/del"
                        android:layout_width="223dp"
                        android:layout_height="18dp"
                        android:layout_marginLeft="7sp"
                        android:orientation="horizontal">


                        <TextView
                            android:id="@+id/tvdelivery"
                            android:layout_width="115dp"
                            android:layout_height="18dp"
                            android:text="hello"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            />

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="20dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_weight="1"
                            android:textAlignment="textEnd"
                            />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="76dp"

                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:paddingTop="3sp"
                            android:text=" 45-60 Mins"
                            android:textSize="10sp"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/textv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7sp"
                            android:text="Discount:"
                            android:textSize="10sp" />

                        <TextView
                            android:id="@+id/textViewrloc"
                            android:layout_width="74dp"
                            android:layout_height="20sp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="8sp"
                            android:elevation="20dp"
                            android:text="TextView"
                            android:textSize="10sp"
                            />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>
</android.support.v7.widget.CardView>


